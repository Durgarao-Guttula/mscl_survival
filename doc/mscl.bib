	pages = {704--9},
	annote = {Poirier, IMarechal, P AEvrard, CGervais, PResearch Support, Non-U.S. Gov'tGermanyApplied microbiology and biotechnologyAppl Microbiol Biotechnol. 1998 Dec;50(6):704-9.},
