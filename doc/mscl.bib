	pages = {137--48},
	annote = {Shabala, LanaBowman, JohnBrown, JanelleRoss, TomMcMeekin, TomShabala, SergeyResearch Support, Non-U.S. Gov'tEnglandEnvironmental microbiologyEnviron Microbiol. 2009 Jan;11(1):137-48. Epub 2008 Sep 12.},
	annote = {The following values have no corresponding Zotero field:auth-address: School of Agricultural Science and Tasmanian Institute of Agricultural Research, University of Tasmania, Private Bag 54, Hobart, Tas 7001, Australia.number: 1edition: 2008/09/17accession-num: 18793315}
