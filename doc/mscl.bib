@article{boucher_mechanosensitive_2009,
	title = {Mechanosensitive closed-closed transitions in large membrane proteins: osmoprotection and tension damping},
	volume = {97},
	issn = {1542-0086 (Electronic) 0006-3495 (Linking)},
	doi = {S0006-3495(09)01447-7 [pii] 10.1016/j.bpj.2009.08.054},
	abstract = {Multiconformation membrane proteins are mechanosensitive (MS) if their conformations displace different bilayer areas. Might MS closed-closed transitions serve as tension buffers, that is, as membrane "spandex"? While bilayer expansion is effectively instantaneous, transitions of bilayer-embedded MS proteins are stochastic (thermally activated) so spandex kinetics would be critical. Here we model generic two-state (contracted/expanded) stochastic spandexes inspired by known bacterial osmovalves (MscL, MscS) then suggest experimental approaches to test for spandex-like behaviors in these proteins. Modeling shows: 1), spandex kinetics depend on the transition state location along an area reaction coordinate; 2), increasing membrane concentration of a spandex right-shifts its midpoint (= tension-Boltzmann); 3), spandexes with midpoints below the activating tension of an osmovalve could optimize osmovalve deployment (required: large midpoint, barrier near the expanded state); 4), spandexes could damp bilayer tension excursions (required: midpoint at target tension, and for speed, barrier halfway between the contracted and expanded states; the larger the spandex Delta-area, the more precise the maintenance of target tension; higher spandex concentrations damp larger amplitude strain fluctuations). One spandex species could not excel as both first line of defense for osmovalve partners and tension damper. Possible interactions among MS closed-closed and closed-open transitions are discussed for MscS- and MscL-like proteins.},
	language = {eng},
	journal = {Biophys J},
	author = {Boucher, P. A. and Morris, C. E. and Joos, B.},
	month = nov,
	year = {2009},
	keywords = {Stochastic Processes, Kinetics, Temperature, *Mechanotransduction, Cellular, *Models, Chemical, Algorithms, Bacterial Proteins/*chemistry, Lipid Bilayers/*chemistry, Membrane Proteins/*chemistry, Stress, Mechanical},
	pages = {2761--70},
	annote = {Boucher, Pierre-AlexandreMorris, Catherine EJoos, BelaCanadian Institutes of Health Research/CanadaResearch Support, Non-U.S. Gov'tUnited StatesBiophysical journalBiophys J. 2009 Nov 18;97(10):2761-70.},
	annote = {The following values have no corresponding Zotero field:auth-address: Institut de Physique, Ottawa-Carleton Campus de l'Universite d'Ottawa, Ottawa, Ontario, Canada.number: 10edition: 2009/11/18accession-num: 19917230}
