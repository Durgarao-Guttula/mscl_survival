	pages = {428--35},
	annote = {Garcia, DManas, PGomez, NRaso, JPagan, RResearch Support, Non-U.S. Gov'tEnglandJournal of applied microbiologyJ Appl Microbiol. 2006 Mar;100(3):428-35.},
