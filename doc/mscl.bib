	pages = {101--6},
	annote = {Poirier, IMarechal, P AGervais, PResearch Support, Non-U.S. Gov'tEnglandJournal of applied microbiologyJ Appl Microbiol. 1997 Jan;82(1):101-6.},
	annote = {The following values have no corresponding Zotero field:auth-address: Laboratoire de Genie des Procedes Alimentaires et Biotechnologiques, ENSBANA, Dijon, France.number: 1edition: 1997/01/01accession-num: 9113880}
