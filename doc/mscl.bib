@article{chiang_gating_2004,
	title = {Gating of the large mechanosensitive channel in situ: estimation of the spatial scale of the transition from channel population responses},
	volume = {86},
	issn = {0006-3495 (Print) 0006-3495 (Linking)},
	doi = {S0006-3495(04)74337-4 [pii] 10.1016/S0006-3495(04)74337-4},
	abstract = {Physical expansion associated with the opening of a tension-sensitive channel has the same meaning as gating charge for a voltage-gated channel. Despite increasing evidence for the open-state conformation of MscL, the energetic description of its complex gating remains incomplete. The previously estimated in-plane expansion of MscL is considerably smaller than predicted by molecular models. To resolve this discrepancy, we conducted a systematic study of currents and dose-response curves for wild-type MscL in \textit{Escherchia coli} giant spheroplasts. Using the all-point histogram method and calibrating tension against the threshold for the small mechanosensitive channel (MscS) in each patch, we found that the distribution of channels among the subconducting states is significantly less dependent on tension than the distribution between the closed and conducting states. At -20 mV, all substates together occupy approximately 30\% of the open time and reduce the mean integral current by approximately 6\%, essentially independent of tension or P(o). This is consistent with the gating scheme in which the major rate-limiting step is the transition between the closed state and a low-conducting substate, and validates both the use of the integral current as a measure of P(o), and treatment of dose-response curves in the two-state approximation. The apparent energy and area differences between the states deltaE and deltaA, extracted from 29 independent dose-response curves, varied in a linearly correlated manner whereas the midpoint tension stayed at approximately 10.4 mN/m. Statistical modeling suggests slight variability of gating parameters among channels in each patch, causing a strong reduction and correlated spread of apparent deltaE and deltaA. The slope of initial parts of activation curves, with a few channels being active, gave estimates of deltaE = 51 +/- 13 kT and deltaA = 20.4 +/- 4.8 nm(2), the latter being consistent with structural models of MscL, which predict deltaA = 23 nm(2).},
	language = {eng},
	journal = {Biophys J},
	author = {Chiang, C. S. and Anishkin, A. and Sukharev, S.},
	month = may,
	year = {2004},
	keywords = {\textit{Escherchia coli} Proteins/*chemistry, Ion Channels/*chemistry, Models, Molecular, Biophysics/methods, Dose-Response Relationship, Drug, Electric Conductivity, Electrophysiology, \textit{Escherchia coli}/*metabolism, Kinetics, Models, Theoretical, Patch-Clamp Techniques, Pressure, Probability, Protein Conformation, Time Factors},
	pages = {2846--61},
	annote = {Chiang, Chien-SungAnishkin, AndriySukharev, SergeiR01NS39314-01/NS/NINDS NIH HHS/United StatesResearch Support, U.S. Gov't, Non-P.H.S.Research Support, U.S. Gov't, P.H.S.United StatesBiophysical journalBiophys J. 2004 May;86(5):2846-61.},
	annote = {The following values have no corresponding Zotero field:auth-address: Department of Biology, University of Maryland College Park, Maryland 20742, USA.number: 5edition: 2004/04/28accession-num: 15111402}
