@article{simonin_sequence_2007,
	title = {Sequence of occurring damages in yeast plasma membrane during dehydration and rehydration: mechanisms of cell death},
	volume = {1768},
	issn = {0006-3002 (Print) 0006-3002 (Linking)},
	doi = {S0005-2736(07)00109-5 [pii] 10.1016/j.bbamem.2007.03.017},
	abstract = {Yeasts are often exposed to variations in osmotic pressure in their natural environments or in their substrates when used in fermentation industries. Such changes may lead to cell death or activity loss. Although the involvement of the plasma membrane is strongly suspected, the mechanism remains unclear. Here, the integrity and functionality of the yeast plasma membrane at different levels of dehydration and rehydration during an osmotic treatment were assessed using various fluorescent dyes. Flow cytometry and confocal microscopy of cells stained with oxonol, propidium iodide, and lucifer yellow were used to study changes in membrane polarization, permeabilization, and endocytosis, respectively. Cell volume contraction, reversible depolarization, permeabilization, and endovesicle formation were successively observed with increasing levels of osmotic pressure during dehydration. The maximum survival rate was also detected at a specific rehydration level, of 20 MPa, above which cells were strongly permeabilized. Thus, we show that the two steps of an osmotic treatment, dehydration and rehydration, are both involved in the induction of cell death. Permeabilization of the plasma membranes is the critical event related to cell death. It may result from lipidic phase transitions in the membrane and from variations in the area-to-volume ratio during the osmotic treatment.},
	language = {eng},
	journal = {Biochim Biophys Acta},
	author = {Simonin, H. and Beney, L. and Gervais, P.},
	month = jun,
	year = {2007},
	keywords = {Osmotic Pressure, Cell Death/*physiology, Cell Membrane/*metabolism, Dehydration/*metabolism, Flow Cytometry, Microscopy, Confocal, Saccharomyces cerevisiae/*physiology},
	pages = {1600--10},
	annote = {Simonin, HeleneBeney, LaurentGervais, PatrickComparative StudyNetherlandsBiochimica et biophysica actaBiochim Biophys Acta. 2007 Jun;1768(6):1600-10. Epub 2007 Mar 28.},
	annote = {The following values have no corresponding Zotero field:auth-address: Laboratoire de Genie des Procedes Alimentaires et Biotechnologiques ENSBANA-1, Esplanade Erasme-21000 DIJON, France.number: 6edition: 2007/05/01accession-num: 17466936}
