@article{sukharev_two_1993,
	title = {Two types of mechanosensitive channels in the {Escherichia} coli cell envelope: solubilization and functional reconstitution},
	volume = {65},
	issn = {0006-3495 (Print) 0006-3495 (Linking)},
	doi = {S0006-3495(93)81044-0 [pii] 10.1016/S0006-3495(93)81044-0},
	abstract = {Mechanosensitive ion channels (MSCs) which could provide for fast osmoregulatory responses in bacteria, remain unidentified as molecular entities. MSCs from \textit{Escherchia coli} (strain AW740) were examined using the patch-clamp technique, either (a) in giant spheroplasts, (b) after reconstitution by fusing native membrane vesicles with asolectin liposomes, or (c) by reassembly of octylglucoside-solubilized membrane extract into asolectin liposomes. MSC activities were similar in all three preparations, consisting of a large nonselective MSC of 3-nS conductance (in 200 mM KCl) that was activated by high negative pressures, and a small weakly anion-selective MSC of 1 nS activated by lower negative pressures. Both channels appeared more sensitive to suction in liposomes than in spheroplasts. After gel filtration of the solubilized membrane extract and reconstituting the fractions, both large MSC and small MSC activities were retrieved in liposomes. The positions of the peaks of channel activity in the column eluate, assayed by patch sampling of individual fractions reconstituted in liposomes, showed an apparent molecular mass under nondenaturing conditions of about 60-80 kDa for the large and 200-400 kDa for the small MSC. We conclude that (a) the large MSC and the small MSC are distinct molecular entities, (b) the fact that both MSCs were functional in liposomes following chromatography strongly suggests that these channels are gated by tension transduced via lipid bilayer, and (c) chromatographic fractionation of detergent-solubilized membrane proteins with subsequent patch sampling of reconstituted fractions can be used to identify and isolate these MS channel proteins.},
	language = {eng},
	journal = {Biophys J},
	author = {Sukharev, S. I. and Martinac, B. and Arshavsky, V. Y. and Kung, C.},
	month = jul,
	year = {1993},
	keywords = {Electrophysiology, Biomechanics, Biophysical Phenomena, Biophysics, Liposomes, Cell Membrane/chemistry/physiology, \textit{Escherchia coli}/*physiology, Ion Channels/chemistry/*physiology, Solubility, Spheroplasts/chemistry/physiology, Water-Electrolyte Balance/physiology},
	pages = {177--83},
	annote = {Sukharev, S IMartinac, BArshavsky, V YKung, CDK 39121/DK/NIDDK NIH HHS/United StatesResearch Support, Non-U.S. Gov'tResearch Support, U.S. Gov't, P.H.S.United statesBiophysical journalBiophys J. 1993 Jul;65(1):177-83.},
