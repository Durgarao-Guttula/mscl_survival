	pages = {32150--7},
	annote = {Blount, PSchroeder, M JKung, CGM47856/GM/NIGMS NIH HHS/United StatesResearch Support, U.S. Gov't, P.H.S.United statesThe Journal of biological chemistryJ Biol Chem. 1997 Dec 19;272(51):32150-7.},
	annote = {The following values have no corresponding Zotero field:auth-address: Laboratory of Molecular Biology, University of Wisconsin, Madison, Wisconsin 53706, USA. plblount@facstaff.wisc.edunumber: 51edition: 1998/01/24accession-num: 9405414}
