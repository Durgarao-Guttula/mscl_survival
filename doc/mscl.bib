	pages = {49--57},
	annote = {Kamaraju, KishoreBelyy, VladislavRowe, IanAnishkin, AndriySukharev, SergeiGM075225/GM/NIGMS NIH HHS/United StatesHoward Hughes Medical Institute/United StatesResearch Support, N.I.H., ExtramuralResearch Support, Non-U.S. Gov'tUnited StatesThe Journal of general physiologyJ Gen Physiol. 2011 Jul;138(1):49-57. doi: 10.1085/jgp.201110606. Epub 2011 Jun 13.},
